git clone https://github.com/trychoosing/fastapi.git
cd fastapi 
apt-get update -y
apt-get install vim -y
pip install -r requirements.txt
mkdir uf
uvicorn main:app --host 0.0.0.0 --port 8000 --timeout-keep-alive 600

redis-server

apt-get update -y
apt-get install vim -y
pip install -r requirements.txt
uvicorn main:app --host 0.0.0.0 --port 8000 --reload --timeout-keep-alive 600