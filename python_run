rm -r fastapi 
git clone https://github.com/trychoosing/fastapi.git
cd fastapi 
mkdir uf
uvicorn main:app --host 0.0.0.0 --port 8000 --reload --timeout-keep-alive 600
apt-get update -y
apt-get install vim -y
pip install -r requirements.txt
uvicorn main:app --host 0.0.0.0 --port 8000 --reload --timeout-keep-alive 600